
<%= @season.name %>
<%= @season.seasonbegin %>
<%= @season.seasonend %>


<%= form_for @season, url: {action: "update"}, html: {method: "patch"} do |f| %>

	<%= f.fields_for :games,@season.games do |game| %>

		<%= game.label game.object.localteam.name %>		
		<%= game.text_field :scorelocalteam %>
		<%= game.label game.object.visitingteam.name %>
		<%= game.text_field :scorevisitingteam %>
		<br>
	<% end %>
	<%= f.submit "Asign Season"%>
<% end %>

<%= @season.name %>
<%= @season.seasonbegin %>
<%= @season.seasonend %>


<%= form_for(@season) do |f| %>
	
	<%= f.fields_for :games do |game| %>
		<%= game.label :localteam_id %>
		<%= game.text_field :scorelocalteam %>

		<%= game.label :visiting_id %>
		<%= game.text_field :scorevisitingteam %>
	<% end %>
	<%= f.submit "Edit Season"%>
<% end %>